{
  "name": "Temporizador",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Extension de Temporizador",
  "permissions": [
    "notifications"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Bienvenido a {nombre del sitio}",
    "default_popup": "popup/popup.html"
  }
}
